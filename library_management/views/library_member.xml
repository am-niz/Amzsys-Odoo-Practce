<odoo>
    <record id="library_member_tree" model="ir.ui.view">
        <field name="name">library.member.tree</field>
        <field name="model">member.model</field>
        <field name="arch" type="xml">
            <field name="member_image"/>
            <field name="member_name"/>
        </field>
    </record>
    <record id="library_member_form" model="ir.ui.view">
        <field name="name">library.member.form</field>
        <field name="model">member.model</field>
        <field name="arch" type="xml">
            <form string="Member Details"
        </field>
    </record>
</odoo>